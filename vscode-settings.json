  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "css.validate": false,
  "editor.quickSuggestions": {
    "strings": true
  },
  "tailwindCSS.includeLanguages": {
    "plaintext": "javascript",
    "html": "html",
    "javascript": "javascript",
    "css": "css"
  },
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
    ["cx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ],
  "explorer.compactFolders": false,
  "editor.minimap.showSlider": "always",
  "editor.semanticHighlighting.enabled": true,
  "editor.linkedEditing": true,
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "editor.wordWrap": "off",
  "diffEditor.wordWrap": "off",
  "[markdown]": {
    "editor.wordWrap": "off",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    }
  },
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "remote.SSH.enableX11Forwarding": false,
  "remote.SSH.showLoginTerminal": true,
  "remote.SSH.remotePlatform": {
    "cron_new": "linux",
    "envoy": "linux",
    "envoy_new": "linux",
    "cron_prod": "linux",
    "cron_dev": "linux",
    "DONT-STARVE-TOGETHER": "linux",
    "report-dashboard.eacoding.com": "linux",
    "report-dashboard.eacoding.com-EC2": "linux",
    "report-dashboard.eacoding.com-VM": "linux",
    "Discord-Bots-EC2": "linux"
  },
  "editor.inlineSuggest.enabled": true,
  "editor.autoClosingBrackets": "always",
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "yaml": true
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.semi": false,
  "workbench.colorTheme": "Dracula Theme",
  "json.schemas": [],
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "powershell.integratedConsole.showOnStartup": false,
  "terminal.integrated.tabs.location": "left",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "settingsSync.ignoredSettings": ["remote.SSH.configFile"],
  "window.commandCenter": true,
  "editor.accessibilitySupport": "off",
  "prettier.singleQuote": true,
  "editor.inlayHints.enabled": "off",
  "editor.guides.indentation": false,
  "[python]": {
    "editor.formatOnType": true
  },
  "github.copilot.advanced": {},
  "git.ignoreMissingGitWarning": true,
  "settingsSync.ignoredExtensions": [],
  "editor.unicodeHighlight.allowedCharacters": {
    "âˆ—": true
  },
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "terminal.integrated.scrollback": 30000,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "github.copilot.editor.enableAutoCompletions": true,
  "typescript.preferences.preferTypeOnlyAutoImports": true,
  "editor.lightbulb.enabled": "off",
  "terminal.integrated.stickyScroll.enabled": false,
  "workbench.tree.indent": 12,
  "workbench.startupEditor": "none",
  "[postcss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "files.associations": {
    "*.mdx": "markdown"
  },
  "editor.guides.bracketPairsHorizontal": true,
  "github.copilot.nextEditSuggestions.enabled": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "editor.stickyTabStops": true,
  "emmet.showAbbreviationSuggestions": false,
  "emmet.showExpandedAbbreviation": "never",
  "workbench.iconTheme": "bearded-icons",
  "security.allowedUNCHosts": ["frsdata20"],
  "workbench.sideBar.location": "right",
  "workbench.activityBar.location": "top"
